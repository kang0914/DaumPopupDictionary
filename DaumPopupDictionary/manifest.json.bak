{
  "manifest_version": 2,

  "name": "Daum Popup Dictionary",
  "description": "Popup Dictionary using Daum Mobile dictionary.",
  "version": "1.0",
  "icons": { "128": "img/3d-dictionary128.png" },
  "permissions": [
    "*://*/*",
    "storage"
  ],
  "browser_action": {
    "default_icon": {
      "16": "img/3d-dictionary16.png",
      "24": "img/3d-dictionary16.png",  
      "32": "img/3d-dictionary16.png" 
    },
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "all_frames": true,
      "matches": [ "\u003Call_urls>" ],
      "js": [
        "contentscripts.js",
        "jquery-ui/jquery-1.12.4.js",
        "jquery-ui/jquery-ui.js"
      ],
      "css": [ "jquery-ui/jquery-ui.css" ]
    }
  ],
  "background": {
    "scripts": [ "background.js" ]
  },
  "commands": {
    "toggle-application-enabled": {
      "suggested_key": {
        "default": "Ctrl+Shift+Z",
        "mac": "Command+Shift+Z"
      },
      "description": "Toggle Application Enabled"
    }
  }
  //"options_ui": {
